import json
import jwt

# Print the version of PyJWT to ensure it is installed correctly
print(jwt.__version__)

# Private key (replace with your actual private key)
private_key = "qwertyuiopasdfghjklzxcvbnmmnbvcxzlkjhgfdsapoiuytrewq"

# Payload
payload = {
    "iss": "10ae94ee-ef60-4a1a-90d3-330cafa1815f",
    "sub": "10ae94ee-ef60-4a1a-90d3-330cafa1815f",
    "aud": "https://fhir.epic.com/interconnect-fhir-oauth/oauth2/authorize",
    "exp": 1622569200,
    "jti": "unique-jwt-id"
}

# Header
headers = {"alg": "RS256", "typ": "JWT"}

# Encode the JWT
token = jwt.encode(
    payload,
    private_key,
    algorithm="RS256",
    headers=headers
)

# Print the token
print(token)
